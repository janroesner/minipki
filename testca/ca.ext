extensions = x509v3

[ x509v3 ]
basicConstraints      = CA:true,pathlen:1
crlDistributionPoints = URI:http://www.cdp.dom/crls/ca.crl
nsCertType            = sslCA,emailCA,objCA
nsCaPolicyUrl         = "http://www.pki.dom/ca/policy.htm"
nsCaRevocationUrl     = "http://www.cdp2.dom/crls/ca.crl"
nsComment             = "MailConf testCA"
